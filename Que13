#include <iostream>
#include<vector>
#include<string>
using namespace std;

int main() {
  int nofcities;
  cout<<"Enter the no of cities :"<<endl;
  cin>> nofcities;
  string arr[nofcities];
  int daygreater;
  for(int i=0;i<nofcities;i++){
      cout<<"Enter the "<<i<<" city"<<endl;
      cin>>arr[i];
      
      
  for(int j=0;j<100;j++){
          int v[2];
    //     //   for(int k=0;k<3;k++){
              int time;
              int timespent;
              int id;
              cout<<"enter time, timespent, id : ";
              cin>>time>>timespent>>id;
              v[0]=time;
              v[1]=timespent;
              v[2]=id;
              if(daygreater<timespent){
                  daygreater==timespent;
              }
              
          }
      }
      if(daygreater>5 && daygreater<12){
          cout<<"morning";
      }else if(daygreater>=12 && daygreater<17){
          cout<<"noon";
      }else if(daygreater>=17 && daygreater<20){
            cout<<"evening";
          
      }else{
          cout<<"night";
      }
      
      return 0;
  }


